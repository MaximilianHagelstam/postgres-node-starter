{
  "name": "ts-node-starter",
  "version": "1.0.0",
  "description": "TypeScript Node.js Starter",
  "main": "dist/server.js",
  "scripts": {
    "start": "export NODE_ENV=production&&node dist/server.js",
    "dev": "export NODE_ENV=development&&nodemon src/server.ts",
    "build": "rm -rf dist&&tsc",
    "test": "jest --forceExit --coverage --verbose",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaximilianHagelstam/ts-node-starter.git"
  },
  "homepage": "https://github.com/MaximilianHagelstam/ts-node-starter#readme",
  "bugs": {
    "url": "https://github.com/MaximilianHagelstam/ts-node-starter/issues"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "mongodb"
  ],
  "author": "Maximilian Hagelstam",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^14.14.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "winston": "^3.3.3"
  }
}

{
  "name": "ts-node-starter",
  "version": "1.0.0",
  "description": "TypeScript Node Starter",
  "main": "dist/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon src/server.ts",
    "build": "tsc",
    "test": "cross-env NODE_ENV=test jest --forceExit --coverage --verbose --detectOpenHandles",
    "start-db": "sudo service postgresql start",
    "setup-db": "ts-node src/config/setupDb.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaximilianHagelstam/ts-node-starter.git"
  },
  "homepage": "https://github.com/MaximilianHagelstam/ts-node-starter#readme",
  "bugs": {
    "url": "https://github.com/MaximilianHagelstam/ts-node-starter/issues"
  },
  "keywords": [
    "typescript",
    "node",
    "postgresql",
    "express"
  ],
  "author": "Maximilian Hagelstam",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.37",
    "@types/pg": "^7.14.11",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.6.0",
    "sequelize": "^6.6.2",
    "winston": "^3.3.3"
  }
}
